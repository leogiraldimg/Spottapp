<p id="notice"><%= notice %></p>

<h1>College Whitelists</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @college_whitelists.each do |college_whitelist| %>
      <tr>
        <td><%= college_whitelist.status %></td>
        <td><%= link_to 'Edit', edit_college_college_whitelist_path(college_whitelist) %></td>
        <td><%= link_to 'Destroy', college_whitelist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>